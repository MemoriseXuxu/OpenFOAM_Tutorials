FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 2567.201;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           $internalField;
    }

    outlet1
    {
        type            zeroGradient;
    }

    outlet2
    {
        type            zeroGradient;
    }

    outlet3
    {
        type            zeroGradient;
    }

    wall-volume_volume
    {
        type            zeroGradient;
    }

}


